    "deny": [
      "Read(.env)",
      "Write(.env)",
      "Edit(.git/**)",
      "Write(.git/**)",
      "Bash(aws s3 cp:*)",
      "Bash(aws s3 mv:*)",
      "Bash(aws s3 rm:*)",
      "Bash(aws s3 sync:*)",
      "Bash(aws s3api put-object:*)",
      "Bash(aws s3api delete-object:*)"
    ],
    "ask": [
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(node:*)"
    ],
    "defaultMode": "default"
  },
  "enableAllProjectMcpServers": false,
  "allowedMcpServers": [
    {
      "serverName": "memory"
    },
    {
      "serverName": "github"
    },
    {
      "serverName": "puppeteer"
    },
    {
      "serverName": "grep-app"
    }
  ],
  "deniedMcpServers": [],
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/session_start.py",
            "timeout": 5
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/user_prompt.py",
            "timeout": 5
          }
        ]
      }
    ],
    "PermissionRequest": [
      {
        "matcher": "Read|Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/permission.py",
            "timeout": 2
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Read|Write|Edit|Bash|Grep|Glob|Task|Skill|WebFetch|LSP",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/pre_tool.py",
            "timeout": 5
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Bash|Grep|Glob|Read|Edit|Write|Task|WebFetch|LSP",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/post_tool.py",
            "timeout": 5
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/stop.py",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/dispatchers/session_end.py",
            "timeout": 10
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/handlers/context_manager.py",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/scripts/diagnostics/statusline.sh",
    "padding": 0
  },
  "enabledPlugins": {
    "pyright@claude-code-lsps": false,
    "clangd-lsp@claude-plugins-official": true
  },
  "outputStyle": "terse",
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true,
    "allowUnsandboxedCommands": true,
    "excludedCommands": [
      "docker",
      "git",
      "gh",
      "npm",
      "node"
    ]
  },
  "spinnerTipsEnabled": false,
  "alwaysThinkingEnabled": true
}
